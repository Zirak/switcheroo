{
    "name" : "Switcheroo",
    "description" : "Fuzzy-navigate between open tabs",
    "version" : "0.1.0",

    "browser_action": {
        "default_title": "Switcheroo",
        "default_popup": "src/popup/popup.html"
    },

    "commands" : {
        "_execute_browser_action" : {
            "suggested_key" : {
                "default"  : "Ctrl+Shift+K"
            },
            "description" : "Do the ol' switcheroo"
        }
    },

    "permissions" : ["tabs"],

    "manifest_version" : 2
}
